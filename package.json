{
  "name": "rivet-react",
  "version": "0.0.0",
  "type": "module",
  "description": "Rivet React components",
  "author": "",
  "license": "BSD-3-Clause",
  "homepage": "http://indiana-university.github.io/rivet-react",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/indiana-university/rivet-react.git"
  },
  "keywords": [
    "react-component"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/rivet-react.cjs",
  "module": "./dist/rivet-react.js",
  "exports": {
    ".": {
      "import": "./dist/rivet-react.js",
      "require": "./dist/rivet-react.cjs"
    },
    "./Accordion": {
      "import": "./dist/rivet-react-accordion.js",
      "require": "./dist/rivet-react-accordion.cjs"
    },
    "./Alerts": {
      "import": "./dist/rivet-react-alert.js",
      "require": "./dist/rivet-react-alert.cjs"
    },
    "./Breadcrumbs": {
      "import": "./dist/rivet-react-breadcrumbs.js",
      "require": "./dist/rivet-react-breadcrumbs.cjs"
    },
    "./Buttons": {
      "import": "./dist/rivet-react-button.js",
      "require": "./dist/rivet-react-button.cjs"
    },
    "./Checkbox": {
      "import": "./dist/rivet-react-checkbox.js",
      "require": "./dist/rivet-react-checkbox.cjs"
    },
    "./Dialog": {
      "import": "./dist/rivet-react-dialog.js",
      "require": "./dist/rivet-react-dialog.cjs"
    },
    "./Dropdown": {
      "import": "./dist/rivet-react-dropdown.js",
      "require": "./dist/rivet-react-dropdown.cjs"
    },
    "./File": {
      "import": "./dist/rivet-react-file.js",
      "require": "./dist/rivet-react-file.cjs"
    },
    "./Footer": {
      "import": "./dist/rivet-react-footer.js",
      "require": "./dist/rivet-react-footer.cjs"
    },
    "./Grid": {
      "import": "./dist/rivet-react-grid.js",
      "require": "./dist/rivet-react-grid.cjs"
    },
    "./Header": {
      "import": "./dist/rivet-react-header.js",
      "require": "./dist/rivet-react-header.cjs"
    },
    "./Input": {
      "import": "./dist/rivet-react-input.js",
      "require": "./dist/rivet-react-input.cjs"
    },
    "./Layouts/AccessDeniedLayout": {
      "import": "./dist/rivet-react-accessdeniedlayout.js",
      "require": "./dist/rivet-react-accessdeniedlayout.cjs"
    },
    "./Layouts/PageNotFoundLayout": {
      "import": "./dist/rivet-react-pagenotfoundlayout.js",
      "require": "./dist/rivet-react-pagenotfoundlayout.cjs"
    },
    "./Layouts/ServerErrorLayoput": {
      "import": "./dist/rivet-react-servererrorlayout.js",
      "require": "./dist/rivet-react-servererrorlayout.cjs"
    },
    "./List": {
      "import": "./dist/rivet-react-list.js",
      "require": "./dist/rivet-react-list.cjs"
    },
    "./LoadingIndicator": {
      "import": "./dist/rivet-react-loadingindicator.js",
      "require": "./dist/rivet-react-loadingindicator.cjs"
    },
    "./PageContent": {
      "import": "./dist/rivet-react-pagecontent.js",
      "require": "./dist/rivet-react-pagecontent.cjs"
    },
    "./Pagination": {
      "import": "./dist/rivet-react-pagination.js",
      "require": "./dist/rivet-react-pagination.cjs"
    },
    "./RadioButton": {
      "import": "./dist/rivet-react-radiobutton.js",
      "require": "./dist/rivet-react-radiobutton.cjs"
    },
    "./SeriesNav": {
      "import": "./dist/rivet-react-seriesnav.js",
      "require": "./dist/rivet-react-seriesnav.cjs"
    },
    "./Table": {
      "import": "./dist/rivet-react-table.js",
      "require": "./dist/rivet-react-table.cjs"
    },
    "./Tabs": {
      "import": "./dist/rivet-react-Tabs.js",
      "require": "./dist/rivet-react-Tabs.cjs"
    },
    "./Subnav": {
      "import": "./dist/rivet-react-subnav.js",
      "require": "./dist/rivet-react-subnav.cjs"
    }
  },
  "scripts": {
    "start": "styleguidist server --config styleguide.config.cjs",
    "dev": "vite",
    "license": "license-check-and-add add -f ./license-check-and-add-config.json",
    "test": "NODE_ENV=test jest",
    "test-win": "jest",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "pretty-quick": "pretty-quick",
    "publish-guide": "npx styleguidist build --config styleguide.config.cjs && cp -r .circleci styleguide && gh-pages --dist styleguide --dotfiles"
  },
  "dependencies": {
    "@react-aria/dialog": "3.5.21",
    "@react-aria/overlays": "3.25.0",
    "@react-aria/utils": "3.27.0",
    "classnames": "2.5.1",
    "focus-trap-react": "11.0.3",
    "rivet-core": "2.8.1",
    "uuid": "11.0.5"
  },
  "peerDependencies": {
    "rivet-icons": "~ 3.0.1",
    "rivet-stickers": "~ 0.3.1",
    "react": ">= 16",
    "react-dom": ">= 16"
  },
  "devDependencies": {
    "@babel/core": "7.26.9",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-react": "7.26.3",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "babel-loader": "9.2.1",
    "core-js": "3.40.0",
    "css-loader": "7.1.2",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "license-check-and-add": "4.0.5",
    "path": "0.12.7",
    "prettier": "3.5.1",
    "pretty-quick": "4.0.0",
    "prop-types": "15.8.1",
    "react-display-name": "0.2.5",
    "react-docgen": "7.1.1",
    "react-styleguidist": "13.1.4",
    "rivet-icons": "3.0.1",
    "rivet-stickers": "0.3.1",
    "semantic-release": "24.2.3",
    "style-loader": "4.0.0",
    "vite": "6.2.3",
    "vite-plugin-externalize-deps": "0.9.0"
  },
  "release": {
    "branches": [
      {
        "name": "master"
      },
      {
        "name": "alpha",
        "channel": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "channel": "beta",
        "prerelease": true
      }
    ]
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && license"
    }
  }
}
