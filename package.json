{
  "name": "rivet-react",
  "version": "0.8.0",
  "description": "Rivet React components",
  "author": "",
  "license": "BSD-3-Clause",
  "homepage": "http://indiana-university.github.io/rivet-react",
  "repository": "https://github.com/indiana-university/rivet-react",
  "keywords": [
    "react-component"
  ],
  "main": "build/dist/components/index.js",
  "types": "build/dist/components/index.d.ts",
  "scripts": {
    "start": "npx styleguidist server",
    "build": "rm -rf ./build/dist && tsc -p tsconfig.dist.json",
    "license": "license-check-and-add",
    "publish-guide": "npx styleguidist build && cp -r .circleci styleguide && gh-pages --dist styleguide --dotfiles",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "react": "16.12.0",
    "react-display-name": "0.2.5",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "2.14.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.4.0",
    "@testing-library/user-event": "8.0.2",
    "@types/classnames": "2.2.9",
    "@types/enzyme": "3.10.4",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.0.25",
    "@types/node": "13.1.2",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "codecov.io": "0.1.6",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "gh-pages": "2.1.1",
    "license-check-and-add": "3.0.3",
    "react-docgen-typescript": "1.16.1",
    "react-scripts": "3.3.0",
    "react-styleguidist": "10.3.2",
    "rivet-collapsible": "0.2.11",
    "rivet-switch": "0.3.2",
    "rivet-uits": "1.7.2",
    "semantic-release": "15.14.0",
    "typescript": "3.7.4"
  },
  "eslintConfig": {
    "extends": ["plugin:@typescript-eslint/recommended"],
    "rules": {
      "@typescript-eslint/type-annotation-spacing": [
        "warn",
        {
          "before": true,
          "after": true
        }
      ],
      "@typescript-eslint/member-delimiter-style": ["warn", {
        "multiline": {
          "delimiter": "comma",
          "requireLast": true
        },
        "singleline": {
          "delimiter": "comma",
          "requireLast": false
        }
      }]
    },
    "overrides": [
      {
        "files": ["**/*.ts?(x)"],
        "rules": {
        }
      }
    ]
  },
  "files": [
    "build/dist/components",
    "addons"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx,ts,tsx}",
      "!src/components/**/index.{ts,tsx}"
    ]
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "license-header.txt",
    "exact_paths_method": "INCLUDE",
    "exact_paths": [
      "addons",
      "src",
      "styleguide.config.js"
    ],
    "file_type_method": "INCLUDE",
    "file_types": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "insert_license": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
