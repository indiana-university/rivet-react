{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "**/*.js",
      "node_modules/**/*.ts"
    ]
  }
  ,
  "rules": {
    "interface-name" : [false, "always-prefix"],
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "no-bitwise": false
  }
}